<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.b6exclusiveautoboutique.fxcontrollers.LoginController">
   <children>
      <VBox alignment="CENTER" layoutX="152.0" layoutY="114.0" prefHeight="400.0" prefWidth="448.0" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="299.0" spacing="25.0">
               <children>
                  <TextField fx:id="nameTextField" alignment="TOP_LEFT" promptText="Name" />
                  <TextField fx:id="surnameTextField" alignment="TOP_LEFT" layoutX="236.0" layoutY="64.0" promptText="Surname" />
               </children>
            </HBox>
            <TextField fx:id="emailTextField" promptText="Email" text="admin@admin.com" />
            <TextField fx:id="passwordTextField" layoutX="10.0" layoutY="232.0" promptText="Password" text="admin" />
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="299.0" spacing="25.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onLoginButtonPressed" text="Login" />
                  <Button mnemonicParsing="false" onAction="#onRegisterButtonPressed" text="Register" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
